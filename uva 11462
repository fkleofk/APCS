	#include<algorithm>
	#include<iostream>
	using namespace std;
	int main() {
		int n ;
		while( cin >> n ) {
			int age[n] ;
			if ( n == 0 )  {
				break ;
			}
			else {
				for ( int i = 0 ; i < n ; i++ ) {
					cin >> age[i] ;
				}
				sort(age,age+n) ;
				for ( int q = 0 ; q < n ; q++ ) {
					if ( q != 0 ) {
						cout << " " ;
					}
					cout << age[q]  ;
				}
			cout << endl ;
			}
		}
		return 0;
	}
